{
  "name": "krown-clothing-backend",
  "version": "0.0.1",
  "engines": {
    "node": "14.18.1",
    "npm": "6.14.15"
  },
  "scripts": {
    "client": "cd client && pnpm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "pushGit": "git push git@github.com:RDZobrist/krown_clothing.git && git status",
    "dev": "concurrently --kill-others-on-fail \"pnpm server\" \"pnpm client\"",
    "start": "node server.js",
    "test-client": "cd client && yarn test"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.21.1",
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-sslify": "1.2.0",
    "http-proxy-middleware": "^2.0.1",
    "stripe": "^6.28.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.19.1"
  }
}
