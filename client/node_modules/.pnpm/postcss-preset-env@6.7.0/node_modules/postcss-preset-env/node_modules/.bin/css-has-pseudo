#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/css-has-pseudo@0.10.0/node_modules/css-has-pseudo/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/css-has-pseudo@0.10.0/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-preset-env@6.7.0/node_modules/css-has-pseudo/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-preset-env@6.7.0/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-preset-env@6.7.0/node_modules/postcss-preset-env/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/css-has-pseudo@0.10.0/node_modules/css-has-pseudo/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/css-has-pseudo@0.10.0/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-preset-env@6.7.0/node_modules/css-has-pseudo/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-preset-env@6.7.0/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-preset-env@6.7.0/node_modules/postcss-preset-env/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../css-has-pseudo/cli.js" "$@"
else
  exec node  "$basedir/../../../css-has-pseudo/cli.js" "$@"
fi
