#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/nanoid@3.1.30/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss@8.3.11/node_modules/nanoid/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss@8.3.11/node_modules/nanoid/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss@8.3.11/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss@8.3.11/node_modules/postcss/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/nanoid@3.1.30/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss@8.3.11/node_modules/nanoid/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss@8.3.11/node_modules/nanoid/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss@8.3.11/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss@8.3.11/node_modules/postcss/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../nanoid/bin/nanoid.cjs" "$@"
fi
