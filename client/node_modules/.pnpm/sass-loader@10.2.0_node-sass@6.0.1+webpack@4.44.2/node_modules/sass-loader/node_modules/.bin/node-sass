#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node-sass@6.0.1/node_modules/node-sass/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node-sass@6.0.1/node_modules/node-sass/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node-sass@6.0.1/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/sass-loader@10.2.0_node-sass@6.0.1+webpack@4.44.2/node_modules/node-sass/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/sass-loader@10.2.0_node-sass@6.0.1+webpack@4.44.2/node_modules/node-sass/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/sass-loader@10.2.0_node-sass@6.0.1+webpack@4.44.2/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/sass-loader@10.2.0_node-sass@6.0.1+webpack@4.44.2/node_modules/sass-loader/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node-sass@6.0.1/node_modules/node-sass/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node-sass@6.0.1/node_modules/node-sass/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node-sass@6.0.1/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/sass-loader@10.2.0_node-sass@6.0.1+webpack@4.44.2/node_modules/node-sass/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/sass-loader@10.2.0_node-sass@6.0.1+webpack@4.44.2/node_modules/node-sass/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/sass-loader@10.2.0_node-sass@6.0.1+webpack@4.44.2/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/sass-loader@10.2.0_node-sass@6.0.1+webpack@4.44.2/node_modules/sass-loader/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node-sass/bin/node-sass" "$@"
else
  exec node  "$basedir/../../../node-sass/bin/node-sass" "$@"
fi
