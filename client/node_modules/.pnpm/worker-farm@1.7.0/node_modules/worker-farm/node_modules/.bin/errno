#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/errno@0.1.8/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/worker-farm@1.7.0/node_modules/errno/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/worker-farm@1.7.0/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/worker-farm@1.7.0/node_modules/worker-farm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/errno@0.1.8/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/worker-farm@1.7.0/node_modules/errno/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/worker-farm@1.7.0/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/worker-farm@1.7.0/node_modules/worker-farm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../errno/cli.js" "$@"
else
  exec node  "$basedir/../../../errno/cli.js" "$@"
fi
