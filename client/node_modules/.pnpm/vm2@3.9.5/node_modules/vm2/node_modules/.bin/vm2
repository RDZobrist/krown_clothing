#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/vm2@3.9.5/node_modules/vm2/bin/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/vm2@3.9.5/node_modules/vm2/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/vm2@3.9.5/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules:/Users/Zobrist/Desktop/Code/node_modules:/Users/Zobrist/Desktop/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/vm2@3.9.5/node_modules/vm2/bin/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/vm2@3.9.5/node_modules/vm2/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/vm2@3.9.5/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules:/Users/Zobrist/Desktop/Code/node_modules:/Users/Zobrist/Desktop/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vm2" "$@"
else
  exec node  "$basedir/../../bin/vm2" "$@"
fi
