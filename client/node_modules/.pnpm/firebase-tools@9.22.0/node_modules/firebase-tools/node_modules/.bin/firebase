#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/firebase-tools@9.22.0/node_modules/firebase-tools/lib/bin/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/firebase-tools@9.22.0/node_modules/firebase-tools/lib/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/firebase-tools@9.22.0/node_modules/firebase-tools/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/firebase-tools@9.22.0/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules:/Users/Zobrist/Desktop/Code/node_modules:/Users/Zobrist/Desktop/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/firebase-tools@9.22.0/node_modules/firebase-tools/lib/bin/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/firebase-tools@9.22.0/node_modules/firebase-tools/lib/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/firebase-tools@9.22.0/node_modules/firebase-tools/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/firebase-tools@9.22.0/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules:/Users/Zobrist/Desktop/Code/node_modules:/Users/Zobrist/Desktop/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin/firebase.js" "$@"
else
  exec node  "$basedir/../../lib/bin/firebase.js" "$@"
fi
