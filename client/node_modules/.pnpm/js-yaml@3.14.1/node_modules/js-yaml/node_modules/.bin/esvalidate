#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/js-yaml@3.14.1/node_modules/esprima/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/js-yaml@3.14.1/node_modules/esprima/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/js-yaml@3.14.1/node_modules/esprima/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/js-yaml@3.14.1/node_modules/esprima/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../../esprima/bin/esvalidate.js" "$@"
fi
