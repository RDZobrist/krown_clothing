#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/acorn@8.5.0/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/bin/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/acorn@8.5.0/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/acorn" "$@"
else
  exec node  "$basedir/../../bin/acorn" "$@"
fi
