#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/browserslist@4.17.5/node_modules/browserslist/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/browserslist@4.17.5/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-reduce-initial@4.0.3/node_modules/browserslist/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-reduce-initial@4.0.3/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-reduce-initial@4.0.3/node_modules/postcss-reduce-initial/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/browserslist@4.17.5/node_modules/browserslist/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/browserslist@4.17.5/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules:/Users/Zobrist/Code/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-reduce-initial@4.0.3/node_modules/browserslist/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-reduce-initial@4.0.3/node_modules:/Users/Zobrist/Code/krown_clothing/node_modules/.pnpm/postcss-reduce-initial@4.0.3/node_modules/postcss-reduce-initial/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../browserslist/cli.js" "$@"
fi
