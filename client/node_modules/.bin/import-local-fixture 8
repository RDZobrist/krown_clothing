#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/import-local/fixtures/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/import-local/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules:/Users/Zobrist/Desktop/Code/node_modules:/Users/Zobrist/Desktop/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/import-local/fixtures/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules/import-local/node_modules:/Users/Zobrist/Desktop/Code/krown_clothing/node_modules:/Users/Zobrist/Desktop/Code/node_modules:/Users/Zobrist/Desktop/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
